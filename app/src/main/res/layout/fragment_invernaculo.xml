<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_invernaculo_fragment"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="mdatum.udc.com.m_datum.encuestaAgroquimicos.InvernaculoFragment"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <!-- TODO: Update blank fragment layout -->

    <EditText
        android:id="@+id/et_cant_modulos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        android:text="@string/et_cant_modulos"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/et_sup_unit"
        android:layout_width="158dp"
        android:layout_height="43dp"
        android:layout_marginBottom="17dp"
        android:layout_marginEnd="55dp"
        android:layout_marginLeft="54dp"
        android:layout_marginRight="55dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        android:text="@string/et_sup_unit"
        app:layout_constraintBottom_toTopOf="@+id/sp_mat_estruct"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/et_cant_modulos"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/tv_anio_const"
        android:layout_width="89dp"
        android:layout_height="36dp"
        android:layout_marginEnd="6dp"
        android:layout_marginRight="6dp"
        android:layout_marginTop="198dp"
        android:text="@string/tv_anio_const"
        app:layout_constraintRight_toLeftOf="@+id/sp_anio_construct"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="21dp"
        android:text="@string/tv_mts2"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_sup_unit"
        app:layout_constraintRight_toRightOf="@+id/et_cant_modulos"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/tv_mat_estruct"
        android:layout_width="0dp"
        android:layout_height="42dp"
        android:layout_marginBottom="66dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:layout_marginTop="68dp"
        android:text="@string/tv_mat_estruct"
        app:layout_constraintBottom_toBottomOf="@+id/tv_anio_const"
        app:layout_constraintLeft_toLeftOf="@+id/et_sup_unit"
        app:layout_constraintRight_toRightOf="@+id/tv_anio_const"
        app:layout_constraintTop_toBottomOf="@+id/et_cant_modulos"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Spinner
        android:id="@+id/sp_mat_estruct"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="3dp"
        android:layout_marginLeft="4dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="3dp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_mat_estruct"
        app:layout_constraintLeft_toLeftOf="@+id/sp_anio_construct"
        app:layout_constraintRight_toRightOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/tv_mat_estruct"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Spinner
        android:id="@+id/sp_anio_construct"
        android:layout_width="118dp"
        android:layout_height="30dp"
        android:layout_marginBottom="66dp"
        android:layout_marginEnd="53dp"
        android:layout_marginLeft="53dp"
        android:layout_marginRight="53dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="66dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_agregar_otro"
        app:layout_constraintLeft_toLeftOf="@+id/btn_agregar_otro"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/sp_mat_estruct"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/btn_agregar_otro"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/btn_agregar_otro"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_anio_const"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />
</android.support.constraint.ConstraintLayout>
