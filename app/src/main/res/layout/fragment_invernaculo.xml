<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cl_invernadero_fragment"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="mdatum.udc.com.m_datum.encuestaAgroquimicos.InvernaculoFragment"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <!-- TODO: Update blank fragment layout -->
    <EditText
        android:id="@+id/et_cant_modulos"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="number"
        android:hint="@string/et_cant_modulos"
        app:layout_constraintRight_toLeftOf="@+id/sp_anio_construct"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="1dp"
        android:layout_marginTop="9dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/et_sup_unit"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="1dp" />

    <EditText
        android:id="@+id/et_sup_unit"
        android:layout_width="184dp"
        android:layout_height="42dp"
        android:layout_marginStart="64dp"
        android:ems="10"
        android:inputType="number"
        android:hint="@string/et_sup_unit"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/tv_mat_estruct"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_cant_modulos"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="26dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="64dp" />

    <TextView
        android:id="@+id/tv_anio_const"
        android:layout_width="89dp"
        android:layout_height="36dp"
        android:layout_marginEnd="63dp"
        android:text="@string/tv_anio_const"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="64dp"
        app:layout_constraintRight_toRightOf="@+id/tv_mat_estruct"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_mat_estruct"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="64dp"
        android:layout_marginRight="63dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="21dp"
        android:text="@string/tv_mts2"
        android:textSize="18sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/sp_mat_estruct"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="64dp"
        android:layout_marginTop="9dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="10dp"
        app:layout_constraintTop_toTopOf="@+id/et_sup_unit"
        app:layout_constraintLeft_toRightOf="@+id/tv_mat_estruct"
        android:layout_marginLeft="62dp"
        app:layout_constraintVertical_bias="0.428" />

    <TextView
        android:id="@+id/tv_mat_estruct"
        android:layout_width="152dp"
        android:layout_height="26dp"
        android:text="@string/tv_mat_estruct"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/et_sup_unit"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/sp_mat_estruct"
        android:layout_width="0dp"
        android:layout_height="30dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/sp_anio_construct"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/sp_anio_construct"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/sp_anio_construct"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="2dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/btn_agregar_otro"
        android:layout_marginTop="71dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="73dp"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        app:layout_constraintLeft_toRightOf="@+id/et_sup_unit"
        android:layout_marginLeft="2dp" />

    <Button
        android:id="@+id/btn_agregar_otro"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_agregar_otro"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp" />

</android.support.constraint.ConstraintLayout>
